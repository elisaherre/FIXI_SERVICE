<!-- app/views/restaurants/new.html.erb -->
<%= form_with model: @service do |f| %>
  <%= f.label :user_id %>
  <%= f.text_field :user_id %>
  <%= f.label :category_id %>
  <%= f.text_field :category_id %>
  <%= f.label :description %>
  <%= f.number_field :description %>
  <%= f.submit %>
<% end %>




<h1>Requests#index</h1>
<p>Find me in app/views/requests/index.html.erb</p>
<% @requests.each do |request| %>
<p><%= request.description %><%= link_to "review", new_request_review_path(request) if request.status == "finalizado" && Review.where(user: current_user, request: request).size == 0 %></p>
<% end %>
